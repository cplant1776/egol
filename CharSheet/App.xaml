<Application
    x:Class="CharSheet.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:cnv="clr-namespace:CharSheet.classes.display"
    xmlns:local="clr-namespace:CharSheet"
    StartupUri="MainWindow.xaml">

    <Application.Resources>
        <ResourceDictionary xmlns:local="using CharSheet;">
            <cnv:XPToProgressConverter x:Key="XPToProgressConverter" />
            <cnv:XPToLevelConverter x:Key="XPToLevelConverter" />
            <cnv:XPToProgressConverterString x:Key="XPToProgressConverterString" />
            <cnv:DeadlineDateTimeToStringConverter x:Key="DeadlineDateTimeToStringConverter" />
            <cnv:AttributeIdToAttributeString x:Key="AttributeIdToAttributeString" />
            <cnv:SkillIdToSkillString x:Key="SkillIdToSkillString" />
            <cnv:PlusButtonName x:Key="PlusButtonName" />
            <cnv:MinusButtonName x:Key="MinusButtonName" />
        </ResourceDictionary>
    </Application.Resources>
</Application>
